{
  "name": "ade-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "ADE Platform for containerized development environments",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest \"src/__tests__/services/.*\\.test\\.ts\"",
    "test:integration": "jest \"src/__tests__/integration/.*\\.test\\.ts\"",
    "test:performance": "jest \"src/__tests__/performance/.*\\.test\\.ts\"",
    "start": "react-scripts start",
    "eject": "react-scripts eject",
    "sync:dev-to-test": "node scripts/sync-dev-to-test.js",
    "sync:test-to-prod": "node scripts/sync-test-to-prod.js",
    "sync:validate": "node scripts/sync-validate.js",
    "sync:backup": "node scripts/sync-backup.js",
    "sync:rollback": "node scripts/sync-rollback.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:build:prod": "docker-compose -f docker-compose.prod.test.yml build",
    "docker:up:prod": "docker-compose -f docker-compose.prod.test.yml up",
    "docker:down:prod": "docker-compose -f docker-compose.prod.test.yml down",
    "docker:validate": "node .cursor/scripts/validate-docker.js",
    "docker:sync-env": "node .cursor/scripts/sync-docker-env.js",
    "precommit": "npm run lint && npm run test && npm run docker:validate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^6.0.0-beta.31",
    "@mui/material": "^5.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3": "^7.4.3",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^1.8.29",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.52.2",
    "notistack": "^3.0.1",
    "openai": "^4.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.30.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.1",
    "redis": "^4.6.13",
    "rxjs": "^7.8.2",
    "sharp": "^0.33.5",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "styled-components": "^6.1.8",
    "tsyringe": "^4.8.0",
    "typescript": "^4.9.5",
    "uuid": "^11.1.0",
    "vosk-browser": "^0.0.8",
    "winston": "^3.11.0",
    "ws": "^8.18.1",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/testing-library": "^0.2.2",
