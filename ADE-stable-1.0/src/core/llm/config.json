{
    "llm": {
        "model": "gpt-4",
        "temperature": 0.2,
        "max_tokens": 1000,
        "top_p": 0.95,
        "frequency_penalty": 0.0,
        "presence_penalty": 0.0,
        "stop": null
    },
    "vector_store": {
        "index_path": "data/error_embeddings.index",
        "dimension": 1536,
        "similarity_threshold": 0.8,
        "max_results": 5,
        "save_interval": 100
    },
    "error_analysis": {
        "min_confidence": 0.7,
        "max_contributing_factors": 5,
        "max_suggested_fixes": 3,
        "context_window": 1000
    }
} 